    // your code here
    var start = age ,gender ,town ,responses;
    age = prompt('Quel age as-tu?');
    gender= prompt('Quel est ton genre?);
    town= prompt('D\'où viens-tu?');
    responses = ('J'ai')+ age +('ans ;' )+ ('je suis') + gender + ('je viens de') + town ; 
    
    if(confirm('Ces réponses sont-elles exactes?'"Oui"')) {
        alert(responses); 
    }
        else {
        alert('<em>re-run the process</em>');
        }
})();
